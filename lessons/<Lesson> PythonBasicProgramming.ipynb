    "Syntax \n",
    "if expression :\n",
    "    code statement 1\n",
    "else:\n",
    "    code statement 2\n",
    "'''\n",
    "a = 10\n",
    "if a >= 10 :\n",
    "    print('Apple')\n",
    "else:\n",
